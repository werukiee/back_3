CREATE TABLE form(
id int(10) unsigned NOT NULL AUTO_INCREMENT,
name VARCHAR(128) NOT NULL DEFAULT '',
email VARCHAR(128) NOT NULL DEFAULT '',
data DATE,
gender char(1),
konech int(10) unsigned NOT NULL,
bio TEXT,
chebox int(10),
PRIMARY KEY(id)
);

CREATE TABLE super(
id int(10) unsigned NOT NULL AUTO_INCREMENT,
super_name varchar(128) NOT NULL,
PRIMARY KEY(id)
);
